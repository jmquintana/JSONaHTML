{
  "name": "JSONaHTML",
  "version": "0.1.0",
  "manifest_version": 2,
  "description": "",
  "homepage_url": "https://web.hakopako.net/",
  "content_scripts": [{
    "matches": ["*://*/*", "file://*/*"],
    "js": ["scripts/jquery.min.js", "scripts/xlsx.full.min.js", "scripts/FileSaver.min.js", "scripts/FileSaver.js", "content.js"],
    "map": ["scripts/FileSaver.min.js.map"],
    "run_at": "document_idle"
  }],
  "web_accessible_resources": [
    "scripts/jquery.min.js", "scripts/xlsx.full.min.js", "scripts/FileSaver.min.js", "scripts/FileSaver.min.js", "scripts/FileSaver.min.js.map", "style.css"
  ],
  "permissions": ["downloads"]
}